{
  "name": "abcjs",
  "version": "6.0.0-beta.36",
  "description": "Renderer for abc music notation",
  "main": "dist/abcjs-basic-min.js",
  "types": "types/index.d.ts",
  "scripts": {
    "webpack": "webpack",
    "build": "npm run build:basic && npm run webpack",
    "build:basic": "npm run webpack -- --config-name basic",
    "build:plugin": "npm run webpack -- --config-name plugin",
    "build:test": "npm run webpack -- --config-name test",
    "build:synth": "npm run webpack -- --config-name synth",
    "test": "mocha 'tests/**/*.js'",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "build:analyze": "npm run build:basic -- --env analyze"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/paulrosen/abcjs.git"
  },
  "keywords": [
    "abc",
    "music",
    "notation",
    "midi",
    "webaudio"
  ],
  "author": "Paul Rosen <paul@paulrosen.net> (https://abcjs.net)",
  "contributors": [
    "Gregory Dyke",
    "Todd Brown",
    "Thomas Chandelle"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/paulrosen/abcjs/issues"
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/paulrosen"
  },
  "homepage": "https://abcjs.net",
  "devDependencies": {
    "@babel/core": "7.16.7",
    "@babel/plugin-transform-runtime": "7.16.7",
    "@babel/preset-env": "7.16.7",
    "@tarp/require": "1.4.3",
    "babel-loader": "8.2.3",
    "chai": "4.3.4",
    "core-js": "3.9.0",
    "mocha": "8.4.0",
    "ts-loader": "9.2.6",
    "typescript": "4.5.4",
    "vuepress": "2.0.0-beta.26",
    "vuex": "4.0.0",
    "webpack-bundle-analyzer": "4.4.2",
    "webpack-cli": "4.9.0"
  }
}
